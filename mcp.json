{
  "mcpServers": {
    "gemini": {
      "command": "uvx",
      "args": [
        "--from", 
        "git+https://github.com/cdecl/gemini-mcp",
        "gemini-mcp"
      ],
      "env": {
        "GEMINI_API_KEY": "YOUR_API_KEY_HERE",
        "GEMINI_MODEL": "gemini-2.5-flash"
      }
    }
  }
}
